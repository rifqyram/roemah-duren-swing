	<parameter name="ORDER_ID" class="java.lang.String"/>
	<parameter name="USER_ID" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT tod.id AS id,
       tod.order_id AS order_id,
       t.po_number AS po_number,
       DATE_FORMAT(t.trans_date, '%d %M %Y') AS trans_date,
       mc.name AS customer_name,
       msp.product_name AS product_name,
       REPLACE(FORMAT(msp.price, 0), ',', '.') AS product_price,
       tod.quantity AS quantity,
       REPLACE(FORMAT(tod.total_price, 0), ',', '.') AS total_price,
       REPLACE(FORMAT(grand_total, 0), ',', '.') AS grand_total
FROM (
         SELECT tod.id,
                tod.order_id,
                tod.quantity,
                mp.supplier_product_id,
                mp.price * tod.quantity AS total_price
         FROM m_product mp
                  JOIN t_order_detail tod ON mp.id = tod.product_id
         WHERE tod.order_id = $P{ORDER_ID}
     ) AS tod
         JOIN m_supplier_product msp ON msp.id = tod.supplier_product_id
         JOIN t_order t ON tod.order_id = t.id
         JOIN m_customer mc ON t.customer_id = mc.id
         CROSS JOIN (
    SELECT SUM(total_price) AS grand_total
    FROM (
             SELECT mp.price * tod.quantity AS total_price
             FROM m_product mp
                      JOIN t_order_detail tod ON mp.id = tod.product_id
             WHERE tod.order_id = $P{ORDER_ID}
         ) AS sub
) AS gt;]]>
	</queryString>
